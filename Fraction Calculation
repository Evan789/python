from fractions import Fraction
a = Fraction(5,4)
b = Fraction(7,16)

print(a + b)
27/16

print(a * b)
35/64

c  = a * b   # field the nominator and denominator
print(c.numerator)
35

print(c.denominator)
64

print(float(c))  # convert fraction into a float
0.546875

x = 3.75
y = Fraction(*x.as_integer_ratio())   # convert a float to fraction
print(y)
15/4
